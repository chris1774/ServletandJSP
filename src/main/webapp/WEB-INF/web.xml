<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>GetMavenController</servlet-name>
  	<display-name>GetMavenController</display-name>
  	<description></description>
  	<servlet-class>com.chris.web.GetMavenController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InsertMavenController</servlet-name>
  	<display-name>InsertMavenController</display-name>
  	<description></description>
  	<servlet-class>com.chris.web.InsertMavenController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>GetMavenController</servlet-name>
  	<url-pattern>/getMaven</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InsertMavenController</servlet-name>
  	<url-pattern>/insertMaven</url-pattern>
  </servlet-mapping>
</web-app>